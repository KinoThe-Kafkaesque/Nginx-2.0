	server
	{
		listen 127.0.0.1:9999;
		server_name localhost;



    	autoindex on;


		root /goinfre/aajaanan/brew/var/www;

		location /content {
			try_files /index22.html /anas =413;
		}

	}
}

url: localhost:9000/content

===================================================================================================

http {



	server
	{
		listen 127.0.0.1:9999;
		server_name localhost;



    	autoindex on;


		root /goinfre/aajaanan/brew/var/www;

		location /content {
			try_files /index22.html /content/anas =413;
		}

		location /anas {
			return 200 "Hello anas";
		}

	}
}

url: localhost:9000/content

===================================================================================================

http {



	server
	{
		listen 127.0.0.1:9999;
		server_name localhost;



    	autoindex on;


		root /goinfre/aajaanan/brew/var/www;

		location /content {
			try_files /index22.html /content/anas/anas.txt =413;
		}

		location /anas {
			return 200 "Hello anas";
		}

	}
}

url: localhost:9000/content